# Environment variables template
# Copy this file to .env and update with your actual values

# Goong Maps API Keys
# Get your keys at: https://account.goong.io
EXPO_PUBLIC_GOONG_MAP_KEY=your_goong_maps_key_here
EXPO_PUBLIC_GOONG_API_KEY=your_goong_api_key_here

# Backend API URL
# Update with your actual backend URL
EXPO_PUBLIC_BACKEND_URL=http://localhost:3000

# App Configuration
EXPO_PUBLIC_LOCATION_TRACKING_INTERVAL=10000
EXPO_PUBLIC_GPS_ACCURACY_THRESHOLD=50
